) => {\n    if(data) {\n      setPokemons(data.pokemons.results);\n    } else if(error) {\n      console.log(error);\n    }\n  }, [loading, error, data])\n\n  useEffect(() => {\n    setGqlVariable({\n      limit: 10,\n      offset: props.curPage*10\n    })\n    refetch();\n  }, [props.curPage])\n\n  const filteredPokemon = pokemons.filter(pokemons => {\n    return pokemons.name.includes(props.searchField);\n  })\n\n  function findPokemon(e) {\n    if(e.key === 'Enter') {\n      window.location.href = \"/detail/\" + e.target.value;\n    }\n  }\n\n  return (\n    <div className=\"pokemon-list-wrapper\">\n      {loading ? <span>Loading...</span> :\n        <>\n          <div className=\"pokemon-list-title\">\n            <span>Pokemon List</span>\n          </div>\n\n          <div className=\"search-field\">\n            <label>\n              Filter:\n              <input className=\"search-field-input\" type=\"search\" placeholder=\"Filter here...\" onChange={props.onSearchChange} value={props.searchField}/>\n            </label>\n          </div>\n          <div className=\"search-field\">\n            <label>\n              Search:\n              <input className=\"search-field-input\" type=\"search\" placeholder=\"Search here...\" onKeyPress={findPokemon}/>\n            </label>\n          </div>\n\n          <div className=\"pokemon-list-card-wrapper\">\n          {filteredPokemon.map((items, i) => (\n            <Link to={\"/detail/\" + items.name} key={i}>\n              <div className=\"pokemon-list-card\">\n                <img width=\"96px\" height=\"96px\" src={items.image} alt={items.name} />\n                <div className=\"pokemon-name\">{items.name}</div>\n                <div className=\"pokemon-count\">\n                  Owned:\n                  {JSON.parse(localStorage.getItem(items.name)) == null\n                    ? \"0\" : JSON.parse(localStorage.getItem(items.name)).count}\n                </div>\n              </div>\n            </Link>\n          ))}\n          </div>\n        </>\n      }\n\n      <PaginationBar />\n\n    </div>\n  );\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(PokemonList);","/Users/johanesronaldo/Documents/pokemon-friends/src/routes/PokemonDetail/index.jsx",[],"/Users/johanesronaldo/Documents/pokemon-friends/src/routes/NotFound/index.jsx",[],"/Users/johanesronaldo/Documents/pokemon-friends/src/routes/MyList/index.jsx",[],"/Users/johanesronaldo/Documents/pokemon-friends/src/redux/module/types.js",[],"/Users/johanesronaldo/Documents/pokemon-friends/src/redux/module/actions.js",[],"/Users/johanesronaldo/Documents/pokemon-friends/src/graphql/index.js",[],"/Users/johanesronaldo/Documents/pokemon-friends/src/components/PaginationBar/index.jsx",[],{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","severity":1,"message":"72","line":49,"column":6,"nodeType":"73","endLine":49,"endColumn":21,"suggestions":"74"},"no-native-reassign",["75"],"no-negated-in-lhs",["76"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'refetch'. Either include it or remove the dependency array.","ArrayExpression",["77"],"no-global-assign","no-unsafe-negation",{"desc":"78","fix":"79"},"Update the dependencies array to be: [props.curPage, refetch]",{"range":"80","text":"81"},[1266,1281],"[props.curPage, refetch]"]